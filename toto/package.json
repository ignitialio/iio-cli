{
  "name": "toto",
  "version": "4.0.0",
  "description": "Ignitial.io Application library",
  "author": "Victor Andritoiu <andritoiu@ignitial.fr>",
  "repository": "git@gitlab.com:iio-core/iio-app-nxt.git",
  "main": "test/index.js",
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "server": "node ./server/index.js",
    "start:dev": "npm-run-all --parallel server serve",
    "start:prod": "npm run build && npm run server",
    "lint": "eslint --ext .js,.vue src",
    "build:image": "docker build --rm -t ignitial/toto:${npm_package_version} . && docker tag ignitial/toto:${npm_package_version} registry.ignitial.io/ignitial/toto:${npm_package_version}",
    "build:dlake:image": "docker build --rm -f Dockerfile.dlake -t ignitial/dlake-toto:${npm_package_version} . && docker tag ignitial/dlake-toto:${npm_package_version} registry.ignitial.io/ignitial/dlake-toto:${npm_package_version}",
    "publish:docker": "npm run build:image && docker push registry.ignitial.io/ignitial/toto:${npm_package_version}",
    "publish:dlake:docker": "npm run build:dlake:image && docker push registry.ignitial.io/ignitial/dlake-toto:${npm_package_version}",
    "clear:babel-cache": "rimraf -rf ./node_modules/.cache/babel-loader/*"
  },
  "dependencies": {
    "@ignitial/iio-app-client": "^4.0.12",
    "@ignitial/iio-app-server": "^4.1.0",
    "babel-polyfill": "^6.26.0",
    "register-service-worker": "^1.6.2",
    "typeface-roboto": "0.0.54",
    "vuex-router-sync": "^5.0.0"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.6.0",
    "@vue/cli-plugin-pwa": "^3.6.0",
    "@vue/cli-service": "^3.6.0",
    "babel-eslint": "^9.0.0",
    "eslint": "^4.19.1",
    "eslint-config-standard": "^10.2.1",
    "eslint-friendly-formatter": "^3.0.0",
    "eslint-plugin-import": "^2.17.2",
    "eslint-plugin-node": "^5.2.0",
    "eslint-plugin-promise": "^3.8.0",
    "eslint-plugin-standard": "^3.1.0",
    "eslint-plugin-vue": "^4.7.1",
    "mongodb": "^3.2.3",
    "npm-run-all": "^4.1.5",
    "vue-template-compiler": "^2.6.10"
  },
  "license": "MIT",
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}
